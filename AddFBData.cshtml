@model  FBLeague.Models.FBData
@{
    ViewBag.Title = "AddFBData";
}



<h2>AddFBData</h2>
@using (Html.BeginForm("SaveUser", "Home"))
{
    <p>
        MatchId: <input type="number" name="MatchId" />
    </p>



    <p>
        TeamName1: <input type="text" name="TeamName1" />
    </p>
    <p>
        TeamName2: <input type="text" name="TeamName2" />
    </p>



    string[] s = { "Win", "Draw" };
    SelectList list = new SelectList(s);
    <p>
        MatchStatus: @Html.DropDownList("dropdownlist1", list,
"SELECT"),
    </p>



    <p>
        WinningTeam: <input type="text" name="WinningTeam" />
    </p>
    <p>
        Points: <input type="number" name="Points" />
    </p>
    <input type="submit" value="Save Match Result" />




    @Html.ActionLink("Edit", "Edit") <span>&nbsp;</span>
    @Html.ActionLink("Delete", "Delete")
    <hr />
}




